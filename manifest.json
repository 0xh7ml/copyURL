{
    "name": "copyURL",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Copy the domain name from the URL",
    "author": "Md Saikat",
    "permissions": [
      "activeTab",
      "<all_urls>",
      "contextMenus"
    ],
    "icons" : {
      "48": "icons/copyURL.png",
      "96": "icons/copyURL.png"
    },
    "background":{
      "scripts" : ["/src/main.js"]
    },
    "browser_action": {
      "default_icon": {
        "19": "icons/copyURL.png",
        "38": "icons/copyURL.png"
      },
      "default_title": "copyURL"
    }
    
  }  